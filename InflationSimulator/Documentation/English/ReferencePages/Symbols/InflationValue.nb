Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"cosineSumLagrangian", "[", 
    RowBox[{"f_", ",", "G_"}], "]"}], "[", 
   RowBox[{"\[CurlyPhi]_", ",", " ", "t_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{
    FractionBox["1", "2"], " ", 
    SuperscriptBox[
     RowBox[{"D", "[", 
      RowBox[{"\[CurlyPhi]", ",", "t"}], "]"}], "2"]}], "-", 
   RowBox[{"4", " ", 
    SuperscriptBox["f", "4"], " ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{"-", "20"}], " ", "G", " ", 
       RowBox[{"(", 
        RowBox[{"1", "-", 
         RowBox[{"Cos", "[", 
          FractionBox["\[CurlyPhi]", 
           RowBox[{
            SqrtBox["2"], " ", "f"}]], "]"}]}], ")"}]}], "+", 
      RowBox[{"20", " ", "G", " ", 
       RowBox[{"(", 
        RowBox[{"1", "-", 
         RowBox[{"Cos", "[", 
          FractionBox[
           RowBox[{"5", " ", "\[CurlyPhi]"}], 
           RowBox[{
            SqrtBox["2"], " ", "f"}]], "]"}]}], ")"}]}], "+", 
      RowBox[{"25", " ", 
       SuperscriptBox["G", "2"], " ", 
       RowBox[{"(", 
        RowBox[{"1", "-", 
         RowBox[{"Cos", "[", 
          FractionBox[
           RowBox[{"5", " ", "\[CurlyPhi]"}], 
           RowBox[{
            SqrtBox["2"], " ", "f"}]], "]"}]}], ")"}]}], "+", 
      RowBox[{"16", " ", 
       RowBox[{"(", 
        RowBox[{"1", "-", 
         RowBox[{"Cos", "[", 
          FractionBox[
           RowBox[{"2", " ", 
            SqrtBox["2"], " ", "\[CurlyPhi]"}], "f"], "]"}]}], ")"}]}], "+", 
      RowBox[{"20", " ", "G", " ", 
       RowBox[{"(", 
        RowBox[{"1", "-", 
         RowBox[{"Cos", "[", 
          FractionBox[
           RowBox[{"2", " ", 
            SqrtBox["2"], " ", "\[CurlyPhi]"}], "f"], "]"}]}], ")"}]}]}], 
     ")"}]}]}]}]], "Input",
