Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   OverscriptBox[
    SubscriptBox["c", "s"], "."], "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      FractionBox["\[DifferentialD]", 
       RowBox[{"\[DifferentialD]", "t"}]], 
      SqrtBox[
       FractionBox[
        RowBox[{
         SubscriptBox["\[PartialD]", 
          OverscriptBox["\[CurlyPhi]", "."]], "p"}], 
        RowBox[{
         SubscriptBox["\[PartialD]", 
          OverscriptBox["\[CurlyPhi]", "."]], "\[Rho]"}]]]}], "\[Congruent]", 
     
     RowBox[{
      FractionBox["\[DifferentialD]", 
       RowBox[{"\[DifferentialD]", "t"}]], 
      SqrtBox[
       FractionBox[
        SubscriptBox["p", 
         OverscriptBox["\[CurlyPhi]", "."]], 
        SubscriptBox["\[Rho]", 
         OverscriptBox["\[CurlyPhi]", "."]]]]}]}], "=", 
    FractionBox[
     RowBox[{
      RowBox[{
       SubscriptBox["\[Rho]", 
        OverscriptBox["\[CurlyPhi]", "."]], 
       OverscriptBox[
        SubscriptBox["p", 
         OverscriptBox["\[CurlyPhi]", "."]], "."]}], "-", 
      RowBox[{
       SubscriptBox["p", 
        OverscriptBox["\[CurlyPhi]", "."]], 
       OverscriptBox[
        SubscriptBox["\[Rho]", 
         OverscriptBox["\[CurlyPhi]", "."]], "."]}]}], 
     RowBox[{"2", 
      SqrtBox[
       RowBox[{
        SubscriptBox["p", 
         OverscriptBox["\[CurlyPhi]", "."]], 
        SubsuperscriptBox["\[Rho]", 
         OverscriptBox["\[CurlyPhi]", "."], "3"]}]]}]]}]}], 
  TraditionalForm]], \
"InlineMath",ExpressionUUID->"27027a13-abfa-4bf1-a362-41257204c7e1"]], \
