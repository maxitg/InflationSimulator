     RowBox[{
      FractionBox[
       RowBox[{"\[PartialD]", "p"}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox[
         OverscriptBox["\[CurlyPhi]", "."], "2"]}]], "/", 
      FractionBox[
       RowBox[{"\[PartialD]", "\[Rho]"}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox[
         OverscriptBox["\[CurlyPhi]", "."], "2"]}]]}]], "=", 
    RowBox[{
     SqrtBox[
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         FractionBox[
          RowBox[{"\[PartialD]", "p"}], 
          RowBox[{"\[PartialD]", 
           OverscriptBox["\[CurlyPhi]", "."]}]], 
         FractionBox[
          RowBox[{"\[PartialD]", 
           OverscriptBox["a", "."]}], 
          RowBox[{"\[PartialD]", 
           SuperscriptBox[
            OverscriptBox["\[CurlyPhi]", "."], "2"]}]]}], ")"}], "/", 
       RowBox[{"(", 
        RowBox[{
         FractionBox[
          RowBox[{"\[PartialD]", "\[Rho]"}], 
          RowBox[{"\[PartialD]", 
           OverscriptBox["\[CurlyPhi]", "."]}]], 
         FractionBox[
          RowBox[{"\[PartialD]", 
           OverscriptBox["\[CurlyPhi]", "."]}], 
          RowBox[{"\[PartialD]", 
           SuperscriptBox[
            OverscriptBox["\[CurlyPhi]", "."], "2"]}]]}], ")"}]}]], "=", 
     RowBox[{
      SqrtBox[
       RowBox[{
        FractionBox[
         RowBox[{"\[PartialD]", "p"}], 
         RowBox[{"\[PartialD]", 
          OverscriptBox["\[CurlyPhi]", "."]}]], "/", 
        FractionBox[
         RowBox[{"\[PartialD]", "\[Rho]"}], 
         RowBox[{"\[PartialD]", 
          OverscriptBox["\[CurlyPhi]", "."]}]]}]], "=", 
      SqrtBox[
       FractionBox[
        RowBox[{
         SubscriptBox["\[PartialD]", 
          OverscriptBox["\[CurlyPhi]", "."]], "p"}], 
        RowBox[{
         SubscriptBox["\[PartialD]", 
          OverscriptBox["\[CurlyPhi]", "."]], "\[Rho]"}]]]}]}]}]}], 
  TraditionalForm]], \
