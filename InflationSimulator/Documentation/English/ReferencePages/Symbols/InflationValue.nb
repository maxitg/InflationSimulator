Cell["\<\
Find experimental consistent parameter sets, and compute effective axion \
decay constants using potential and Hubble parameter methods. The following \
computation takes a while:\
\>", "ExampleText",
 CellID->66880141,ExpressionUUID->"dc342702-fd11-4370-bb9b-0214bd84cede"],

Cell[BoxData[
 RowBox[{
  RowBox[{"cosineSumResults", "=", 
   RowBox[{"ParallelMap", "[", 
    RowBox[{
     RowBox[{"Function", "[", 
      RowBox[{
       RowBox[{"{", "input", "}"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"ExperimentallyConsistentInflationQ", "[", "##", "]"}], 
           ",", 
           RowBox[{"Join", "[", 
            RowBox[{"input", ",", 
             RowBox[{"Association", "@", 
              RowBox[{"Thread", "[", 
               RowBox[{
                RowBox[{"{", 
                 RowBox[{"\"\<fDynamic\>\"", ",", "\"\<fStatic\>\""}], "}"}], 
                "\[Rule]", 
                RowBox[{"InflationValue", "[", 
                 RowBox[{"##", ",", 
                  RowBox[{"{", 
                   RowBox[{
                    RowBox[{"InflationProperty", "[", 
                    RowBox[{
                    "\"\<EffectiveAxionDecayConstant\>\"", ",", 
                    "\"\<HorizonExit\>\"", ",", 
                    RowBox[{
                    "Method", "\[Rule]", "\"\<FromHubbleParameter\>\""}]}], 
                    "]"}], ",", 
                    RowBox[{"InflationProperty", "[", 
                    RowBox[{
                    "\"\<EffectiveAxionDecayConstant\>\"", ",", 
                    "\"\<HorizonExit\>\"", ",", 
                    RowBox[{"Method", "\[Rule]", "\"\<FromPotential\>\""}]}], 
                    "]"}]}], "}"}]}], "]"}]}], "]"}]}]}], "]"}], ",", 
           "Nothing"}], "]"}], "&"}], "[", 
        RowBox[{
         RowBox[{
          RowBox[{"cosineSumLagrangian", "[", 
           RowBox[{
            RowBox[{"input", "[", "\"\<fTrue\>\"", "]"}], ",", 
            RowBox[{"input", "[", "\"\<G\>\"", "]"}]}], "]"}], "[", 
          RowBox[{
           RowBox[{"\[CurlyPhi]", "[", "t", "]"}], ",", "t"}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"\[CurlyPhi]", "[", "t", "]"}], ",", 
           RowBox[{"input", "[", "\"\<fieldInitial\>\"", "]"}], ",", "0.0"}], 
          "}"}], ",", "t", ",", 
         RowBox[{"input", "[", "\"\<pivotEfoldingsCount\>\"", "]"}]}], 
        "]"}]}], "]"}], ",", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"With", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"fValue", "=", 
            RowBox[{"RandomReal", "[", 
             RowBox[{"{", 
              RowBox[{"0", ",", "1"}], "}"}], "]"}]}], ",", 
           RowBox[{"G", "=", 
            RowBox[{"RandomReal", "[", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"-", "0.88931"}], ",", 
               RowBox[{"-", "0.88920"}]}], "}"}], "]"}]}], ",", 
           RowBox[{"nPivot", "=", 
            RowBox[{"RandomReal", "[", 
             RowBox[{"{", 
              RowBox[{"50", ",", "60"}], "}"}], "]"}]}]}], "}"}], ",", 
         RowBox[{"\[LeftAssociation]", 
          RowBox[{
           RowBox[{"\"\<fTrue\>\"", "\[Rule]", "fValue"}], ",", 
           RowBox[{"\"\<G\>\"", "\[Rule]", "G"}], ",", 
           RowBox[{"\"\<fieldInitial\>\"", "\[Rule]", 
            RowBox[{"RandomReal", "[", 
             RowBox[{"{", 
              RowBox[{"0", ",", 
               RowBox[{"\[Pi]", " ", "fValue", " ", 
                SqrtBox["2"]}]}], "}"}], "]"}]}], ",", 
           RowBox[{"\"\<pivotEfoldingsCount\>\"", "\[Rule]", "nPivot"}]}], 
          "\[RightAssociation]"}]}], "]"}], ",", "10000"}], "]"}]}], "]"}]}], 
  ";"}]], "Input",
 CellLabel->"In[517]:=",
 CellID->511008118,ExpressionUUID->"3bab9f9a-3d84-4cda-8939-02e71abb4677"],

