Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["V", "NI"], "(", "\[Phi]", ")"}], "=", 
   RowBox[{
    RowBox[{"\[Lambda]", " ", 
     RowBox[{"(", 
      RowBox[{"1", "-", 
       RowBox[{"cos", "(", 
        FractionBox[
         SubscriptBox["\[Phi]", "0"], 
         SubscriptBox["f", "e"]], ")"}]}], ")"}]}], "+", 
    RowBox[{
     FractionBox["\[Lambda]", 
      SubscriptBox["f", "e"]], 
     RowBox[{"sin", "(", 
      FractionBox[
       SubscriptBox["\[Phi]", "0"], 
       SubscriptBox["f", "e"]], ")"}], 
     RowBox[{"(", 
      RowBox[{"\[Phi]", "-", 
       SubscriptBox["\[Phi]", "0"]}], ")"}]}], "+", 
    RowBox[{
     FractionBox["\[Lambda]", 
      RowBox[{"2", 
       SubsuperscriptBox["f", "e", "2"]}]], 
     RowBox[{"cos", "(", 
      FractionBox[
       SubscriptBox["\[Phi]", "0"], 
       SubscriptBox["f", "e"]], ")"}], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"\[Phi]", "-", 
        SubscriptBox["\[Phi]", "0"]}], ")"}], "2"]}], "+", 
    SuperscriptBox[
     RowBox[{"\[ScriptCapitalO]", "(", 
      RowBox[{"\[Phi]", "-", 
       SubscriptBox["\[Phi]", "0"]}], ")"}], "3"]}]}], 
  TraditionalForm]], \
"InlineMath",ExpressionUUID->"31ec49bd-807d-4b78-9a3e-071ec970e94e"]], \
"ExampleText",
 TextAlignment->Center,
 CellID->2124738820,ExpressionUUID->"fb86e95d-a452-42a1-b4bc-d93fb3e54be9"],

Cell[TextData[{
