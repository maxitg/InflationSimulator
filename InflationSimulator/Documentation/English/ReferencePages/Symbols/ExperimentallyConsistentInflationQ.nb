Cell[CellGroupData[{

Cell["ExperimentallyConsistentInflationQ", "ObjectName",
 CellID->1224892054,ExpressionUUID->"d2fb5ad1-866b-4467-b055-2f50ee2f660c"],

Cell[TextData[{
 Cell["   ", "ModInfo",ExpressionUUID->"34d2f922-d5e9-42a0-a84c-e8e25f0872ed"],
 Cell[BoxData[
  RowBox[{"ExperimentallyConsistentInflationQ", "[", 
   RowBox[{
    StyleBox["\[ScriptCapitalL]", "TI"], ",", " ", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       StyleBox["\[CurlyPhi]", "TI"], "[", 
       StyleBox["t", "TI"], "]"}], ",", " ", 
      SubscriptBox[
       StyleBox["\[CurlyPhi]", "TI"], 
       StyleBox["0", "TR"]], ",", " ", 
      RowBox[{
       SubscriptBox[
        StyleBox["\[PartialD]", "TI"], 
        StyleBox["t", "TI"]], 
       SubscriptBox[
        StyleBox["\[CurlyPhi]", "TI"], 
        StyleBox["0", "TR"]]}]}], "}"}], ",", " ", 
    StyleBox["t", "TI"], ",", " ", 
    SubscriptBox[
     StyleBox["N", "TI"], 
     StyleBox["pivot", "TI"]]}], "]"}]], "InlineFormula",ExpressionUUID->
  "290b6777-b08c-4618-ac8f-9b715f337701"],
 "\[LineSeparator]yields ",
 ButtonBox["True",
  BaseStyle->"Link",
  ButtonData->"paclet:ref/True"],
 " if the specified model results in inflation with experimentally consistent \
observables, and ",
 ButtonBox["False",
  BaseStyle->"Link",
  ButtonData->"paclet:ref/False"],
 " otherwise.\n",
 Cell["      ", "ModInfo",ExpressionUUID->
  "e98b6b73-8cb5-4ba9-9b92-a634404f613d"],
 Cell[BoxData[
  RowBox[{"ExperimentallyConsistentInflationQ", "[", 
   RowBox[{
    SubscriptBox[
     StyleBox["n", "TI"], 
     StyleBox["s", "TI"]], ",", " ", 
    StyleBox["r", "TI"]}], "]"}]], "InlineFormula",ExpressionUUID->
  "fda44728-8f15-4d9c-993a-b91f5b13ec82"],
 "\[LineSeparator]yields ",
 ButtonBox["True",
  BaseStyle->"Link",
  ButtonData->"paclet:ref/True"],
 " if a given index of scalar spectral perturbations ",
 Cell[BoxData[
  SubscriptBox[
   StyleBox["n", "TI"], 
   StyleBox["s", "TI"]]], "InlineFormula",
  FormatType->"StandardForm",ExpressionUUID->
  "6583654b-8cf6-446b-9888-33bfc7259139"],
 " and tensor-to-scalar power spectrum ratio ",
 StyleBox["r", "TI"],
 " are consistent with experimental bounds, and ",
 ButtonBox["False",
  BaseStyle->"Link",
  ButtonData->"paclet:ref/False"],
 " otherwise."
}], "Usage",
 CellID->982511436,ExpressionUUID->"6db51ebd-6d1a-4d43-88ae-1ea21ecfaab5"],

Cell[TextData[{
 "If Lagrangian ",
 StyleBox["\[ScriptCapitalL]", "TI"],
 " is supplied, ",
 ButtonBox["True",
  BaseStyle->"Link",
  ButtonData->"paclet:ref/True"],
 " will only be yielded if inflation stops and produces sufficient number of \
e-foldings."
}], "Notes",
 CellID->1067943069,ExpressionUUID->"827e7ab9-a889-48df-a4fb-c2a180da389e"],

Cell[TextData[{
 "Planck TT,TE,EE+lowP at 95% CL from Figure 12 of ",
 ButtonBox["[Ade, P. A. R., et al. \"Planck 2015 results-XX. Constraints on \
inflation.\" Astronomy & Astrophysics 594 (2016): A20, arXiv:1502.02114v2]",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["https://arxiv.org/abs/1502.02114"], None},
  ButtonNote->"https://arxiv.org/abs/1502.02114"],
 " is used for experimental bounds."
}], "Notes",
 CellID->2907726,ExpressionUUID->"9cfb1d3f-a64a-422a-8ae4-83011e3c3f77"]
}, Open  ]],

