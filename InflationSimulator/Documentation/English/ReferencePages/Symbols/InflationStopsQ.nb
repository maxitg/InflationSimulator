Cell["\<\
Determine if inflation produced by a Natural Inflation model stops:\
\>", "ExampleText",
 CellID->983100344,ExpressionUUID->"bb4234d7-4b49-4f38-b0a9-03dec811f5dc"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"InflationStopsQ", "[", 
  RowBox[{
   RowBox[{
    RowBox[{
     FractionBox["1", "2"], 
     SuperscriptBox[
      RowBox[{
       RowBox[{"\[CurlyPhi]", "'"}], "[", "t", "]"}], "2"]}], "-", 
    RowBox[{"(", 
     RowBox[{"1", "-", 
      RowBox[{"Cos", "[", 
       RowBox[{
        RowBox[{"\[CurlyPhi]", "[", "t", "]"}], "/", "10"}], "]"}]}], ")"}]}],
    ",", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"\[CurlyPhi]", "[", "t", "]"}], ",", 
     RowBox[{"0.9", " ", "10", " ", "\[Pi]"}], ",", "0."}], "}"}], ",", "t"}],
   "]"}]], "Input",
 CellLabel->"In[505]:=",
 CellID->1525454868,ExpressionUUID->"27178f9a-c02d-4d9e-8318-4a038f67c6a4"],

Cell[BoxData["True"], "Output",
 CellLabel->"Out[505]=",
 CellID->838110741,ExpressionUUID->"33cb8fdc-92dc-4d9e-8408-56964e8a0009"]
}, Open  ]]
}, Open  ]],

