Cell["\<\
Below are some details on how various properties are computed.\
\>", "Notes",
 CellID->172163674,ExpressionUUID->"305696f0-5b4d-4175-834c-d99e31688f64"],

Cell[TextData[{
 StyleBox["\"SlowRollEpsilon\"", "InlineCode"],
 " is computed using the following equation:"
}], "Notes",
 CellID->1946269614,ExpressionUUID->"160a3eda-c715-40ba-b8ec-de46e1d891a6"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{"\[Epsilon]", "=", 
   RowBox[{
    FractionBox["1", 
     RowBox[{"2", 
      SuperscriptBox["V", "2"]}]], 
    FractionBox[
     RowBox[{"\[PartialD]", "V"}], 
     RowBox[{"\[PartialD]", 
      SuperscriptBox["\[CurlyPhi]", "i"]}]], 
    SuperscriptBox["g", 
     RowBox[{"i", " ", "j"}]], 
    FractionBox[
     RowBox[{"\[PartialD]", "V"}], 
     RowBox[{"\[PartialD]", 
      SuperscriptBox["\[CurlyPhi]", "j"]}]]}]}], 
  TraditionalForm]], \
"InlineMath",ExpressionUUID->"b1a3549a-d3ec-49c0-b516-1e7685f43754"]], \
"ExampleText",
 TextAlignment->Center,
 CellID->1359661814,ExpressionUUID->"3827726d-c15e-4fbb-8c76-5b60f367fae9"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  SuperscriptBox[
   StyleBox["g", "TI"], 
   StyleBox[
    RowBox[{"i", " ", "j"}], "TI"]]], "InlineFormula",ExpressionUUID->
  "5fe990d9-3e5d-4d8e-8421-b77d5c28ea54"],
 " is an inverse of the metric in field space, and summation is implied:"
}], "ExampleText",
 CellID->611355718,ExpressionUUID->"50fd8c0a-c6b4-4a9b-9243-08768049b246"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["g", 
    RowBox[{"i", " ", "j"}]], "=", 
   FractionBox[
    RowBox[{
     SuperscriptBox["\[PartialD]", "2"], 
     StyleBox["\[ScriptCapitalL]", "TI"]}], 
    RowBox[{
     RowBox[{"\[PartialD]", 
      SubscriptBox[
       OverscriptBox["\[CurlyPhi]", "."], "i"]}], 
     RowBox[{"\[PartialD]", 
      SubscriptBox[
       OverscriptBox["\[CurlyPhi]", "."], "j"]}]}]]}], 
  TraditionalForm]], \
"InlineMath",ExpressionUUID->"7e3f645f-1af2-4559-9815-50027942eb11"]], \
"ExampleText",
 TextAlignment->Center,
 CellID->1746252994,ExpressionUUID->"3c8628c4-648c-45be-b35f-7a9f24dbb25d"],

Cell[TextData[{
 "and ",
 Cell[BoxData[
  StyleBox["V", "TI"]], "InlineFormula",ExpressionUUID->
  "e7eb3189-b6bf-4594-9c12-6c4aa6bac69b"],
 " is a potential:"
}], "ExampleText",
 CellID->2026779928,ExpressionUUID->"57ddc332-d2a3-4251-9aee-d7461cf20ce2"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{"V", "=", 
   RowBox[{
    RowBox[{"-", 
     StyleBox["\[ScriptCapitalL]", "TI"]}], 
    StyleBox[
     StyleBox[
      RowBox[{"/", "."}]], "TI"], 
    RowBox[{
     StyleBox["(", "TI"], 
     RowBox[{
      OverscriptBox[
       StyleBox["\[CurlyPhi]", "TI"], "."], "\[Rule]", "0"}], ")"}]}]}], 
  TraditionalForm]], \
"InlineMath",ExpressionUUID->"77c20785-65e8-45af-8c49-efa5106365f0"]], \
"ExampleText",
 TextAlignment->Center,
 CellID->458842689,ExpressionUUID->"f484154e-fc52-4824-81dc-76c2159c0715"],

Cell[TextData[{
 StyleBox["\"SlowRollEta\"", "InlineCode"],
 " is computed according to:"
}], "Notes",
 CellID->354756080,ExpressionUUID->"8ad53efc-069c-4817-a951-be3e713cadbb"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{"\[Eta]", "=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      FractionBox[
       RowBox[{"\[PartialD]", "V"}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["\[CurlyPhi]", "i"]}]], 
      RowBox[{
       SuperscriptBox["g", 
        RowBox[{"i", " ", "j"}]], "(", 
       RowBox[{
        FractionBox[
         RowBox[{
          SuperscriptBox["\[PartialD]", "2"], "V"}], 
         RowBox[{
          RowBox[{"\[PartialD]", 
           SuperscriptBox["\[CurlyPhi]", "j"]}], 
          RowBox[{"\[PartialD]", 
           SuperscriptBox["\[CurlyPhi]", "k"]}]}]], "-", 
        RowBox[{
         SubsuperscriptBox["\[CapitalGamma]", 
          RowBox[{"j", " ", "k"}], "l"], 
         FractionBox[
          RowBox[{"\[PartialD]", "V"}], 
          RowBox[{"\[PartialD]", 
           SuperscriptBox["\[CurlyPhi]", "l"]}]]}]}], ")"}], 
      SuperscriptBox["g", 
       RowBox[{"k", " ", "m"}]], 
      FractionBox[
       RowBox[{"\[PartialD]", "V"}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["\[CurlyPhi]", "m"]}]]}], ")"}], "/", 
    RowBox[{"(", 
     RowBox[{"V", " ", 
      FractionBox[
       RowBox[{"\[PartialD]", "V"}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["\[CurlyPhi]", "a"]}]], 
      SuperscriptBox["g", 
       RowBox[{"a", " ", "b"}]], 
      FractionBox[
       RowBox[{"\[PartialD]", "V"}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["\[CurlyPhi]", "b"]}]]}], ")"}]}]}], 
  TraditionalForm]], \
"InlineMath",ExpressionUUID->"92db2281-abe9-43fd-a0ba-6bf019039713"]], \
"ExampleText",
 TextAlignment->Center,
 CellID->378284598,ExpressionUUID->"21c595fe-8946-4c40-9489-bb4f37949cc5"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  StyleBox["\[CapitalGamma]", "TI"]], "InlineFormula",ExpressionUUID->
  "63a2484c-20fb-4465-9404-cf8d300f11ab"],
 " is the Christoffel connection computed from the metric ",
 Cell[BoxData[
  StyleBox["g", "TI"]], "InlineFormula",ExpressionUUID->
  "a3078af3-8cd7-4625-b0ff-a476183ca73f"],
 "."
}], "ExampleText",
 CellID->621462858,ExpressionUUID->"a23d9aa5-0817-4772-88da-6558f68b8f6a"],

Cell[TextData[{
 StyleBox["\"EffectiveAxionDecayConstant\"", "InlineCode"],
 " is computed as:"
}], "Notes",
 CellID->1494044643,ExpressionUUID->"550c067a-12af-42e3-a954-983ce1249814"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["f", "e"], "=", 
   FractionBox["1", 
    SqrtBox[
     RowBox[{"2", 
      RowBox[{"(", 
       RowBox[{"\[Epsilon]", "-", "\[Eta]"}], ")"}]}]]]}], 
