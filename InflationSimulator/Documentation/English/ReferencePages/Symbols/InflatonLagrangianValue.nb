Cell[TextData[{
 Cell["   ", "ModInfo",ExpressionUUID->"d25befca-1a68-4d81-93d0-2684d2e598d7"],
 Cell[BoxData[
  RowBox[{"InflatonLagrangianValue", "[", 
   RowBox[{
    StyleBox["\[ScriptCapitalL]", "TI"], ",", " ", 
    RowBox[{"{", 
     RowBox[{
      StyleBox["\[CurlyPhi]", "TI"], ",", " ", 
      SubscriptBox[
       StyleBox["\[CurlyPhi]", "TI"], 
       StyleBox["0", "TR"]]}], "}"}], ",", " ", 
    StyleBox["t", "TI"], ",", " ", 
    StyleBox["property", "TI"]}], "]"}]], "InlineFormula",ExpressionUUID->
  "4585a1c2-04d1-472f-9437-3263ecff5c96"],
 " \[LineSeparator]yields the value of a specified ",
 StyleBox["property", "TI"],
 " of Lagrangian ",
 StyleBox["\[ScriptCapitalL]", "TI"],
 " of the field ",
 Cell[BoxData[
  StyleBox[
   RowBox[{"\[CurlyPhi]", 
    RowBox[{"(", "t", ")"}]}], "TI"]], "InlineFormula",ExpressionUUID->
  "59ea7563-3b57-420e-8840-a683cefcafee"],
 " assuming the field takes the value ",
 Cell[BoxData[
  RowBox[{
   StyleBox["\[CurlyPhi]", "TI"], 
   StyleBox["=", "TI"], 
   SubscriptBox[
    StyleBox["\[CurlyPhi]", "TI"], 
    StyleBox["0", "TR"]]}]], "InlineFormula",ExpressionUUID->
  "762e98ef-9b05-4f1e-a0b2-12333589f06d"],
 ".\n",
 Cell["   ", "ModInfo",ExpressionUUID->"8ae30d41-75d9-4162-b605-f054e5570b9d"],
 Cell[BoxData[
  RowBox[{"InflatonLagrangianValue", "[", 
   RowBox[{
    StyleBox["\[ScriptCapitalL]", "TI"], ",", " ", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        StyleBox[
         SubscriptBox[
          StyleBox["\[CurlyPhi]", "TI"], 
          StyleBox["1", "TR"]], "TI"], ",", " ", 
        SubscriptBox[
         StyleBox["\[CurlyPhi]", "TI"], 
         StyleBox[
          RowBox[{"1", ",", " ", "0"}], "TR"]]}], "}"}], ",", " ", 
      RowBox[{"{", 
       RowBox[{
        StyleBox[
         SubscriptBox[
          StyleBox["\[CurlyPhi]", "TI"], 
          StyleBox["2", "TR"]], "TI"], ",", " ", 
        SubscriptBox[
         StyleBox["\[CurlyPhi]", "TI"], 
         StyleBox[
          RowBox[{"2", ",", " ", "0"}], "TR"]]}], "}"}], ",", " ", 
      StyleBox["\[Ellipsis]", "TR"]}], "}"}], ",", " ", 
    StyleBox["t", "TI"], ",", " ", 
    StyleBox["property", "TI"]}], "]"}]], "InlineFormula",ExpressionUUID->
  "48a0363b-10ba-4905-b10a-461fc11e18a0"],
 " \[LineSeparator]yields the value of a specified ",
 StyleBox["property", "TI"],
 " for a multiple field Lagrangian."
}], "Usage",
 CellID->982511436,ExpressionUUID->"17ede818-089a-42bd-8f81-be41f80054d0"],

