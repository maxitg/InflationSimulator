Cell["Evaluate field density of a non-supersymmetric DBI field:", \
"ExampleText",
 CellID->49483078,ExpressionUUID->"08470f69-3640-4afb-9eb3-bf4912e0e20c"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"dbiPressure", "=", 
  RowBox[{"FullSimplify", "[", 
   RowBox[{"InflatonPressure", "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"-", 
        SuperscriptBox[
         RowBox[{"f", "[", "\[CurlyPhi]", "]"}], 
         RowBox[{"-", "1"}]]}], 
       RowBox[{"(", 
        RowBox[{
         SqrtBox[
          RowBox[{"1", "-", 
           RowBox[{
            RowBox[{"f", "[", "\[CurlyPhi]", "]"}], 
            SuperscriptBox["\[CurlyPhi]t", "2"]}]}]], "-", "1"}], ")"}]}], 
      "-", 
      RowBox[{"V", "[", "\[CurlyPhi]", "]"}]}], ",", "\[CurlyPhi]t"}], "]"}], 
   "]"}]}]], "Input",
 CellLabel->"In[21]:=",
 CellID->392585504,ExpressionUUID->"73c147cb-ad35-471e-89f8-254dd7257e8d"],

Cell[BoxData[
 RowBox[{"-", 
  FractionBox[
   RowBox[{
    RowBox[{"-", "1"}], "+", 
    SqrtBox[
     RowBox[{"1", "-", 
      RowBox[{
       SuperscriptBox["\[CurlyPhi]t", "2"], " ", 
       RowBox[{"f", "[", "\[CurlyPhi]", "]"}]}]}]], "+", 
    RowBox[{
     RowBox[{"f", "[", "\[CurlyPhi]", "]"}], " ", 
     RowBox[{"V", "[", "\[CurlyPhi]", "]"}]}]}], 
   RowBox[{"f", "[", "\[CurlyPhi]", "]"}]]}]], "Output",
 CellLabel->"Out[21]=",
 CellID->1846668581,ExpressionUUID->"b9f56b52-b425-4b27-97ad-8018f2707403"]
}, Open  ]],

Cell[TextData[{
 "which is equivalent to equation 3.2 of ",
 ButtonBox["[Michal Spalinski, \"On Power Law Inflation in DBI Models\", \
arXiv:hep-th/0702196v2]",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["https://arxiv.org/abs/hep-th/0702196"], None},
  ButtonNote->"https://arxiv.org/abs/hep-th/0702196"],
 "."
}], "ExampleText",
 CellID->1186968378,ExpressionUUID->"af8b5184-1564-4d78-876d-144ed33570a5"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Reduce", "[", 
  RowBox[{"dbiPressure", "\[Equal]", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"\[Gamma]", "-", "1"}], 
       RowBox[{
        RowBox[{"f", "[", "\[CurlyPhi]", "]"}], " ", "\[Gamma]"}]], "-", 
      RowBox[{"V", "[", "\[CurlyPhi]", "]"}]}], "/.", 
     RowBox[{"\[Gamma]", "\[Rule]", 
      FractionBox["1", 
       SqrtBox[
        RowBox[{"1", "-", 
         RowBox[{
          RowBox[{"f", "[", "\[CurlyPhi]", "]"}], 
          SuperscriptBox["\[CurlyPhi]t", "2"]}]}]]]}]}], ")"}]}], 
  "]"}]], "Input",
 CellLabel->"In[22]:=",
 CellID->542343200,ExpressionUUID->"5e52a4a3-8f5f-4e0a-a080-d914ef8d71c0"],

Cell[BoxData["True"], "Output",
 CellLabel->"Out[22]=",
 CellID->287655037,ExpressionUUID->"7adff4c8-ea13-4a37-95b2-378a948ef39f"]
}, Open  ]]
}, Open  ]]
}, Open  ]],

